                    xmlns:converter="clr-namespace:picibird.hbs.wpf.converter">

    <converter:StringFormCConverter x:Key="FormCConverter" />
    <converter:CollapsedIfNoDataConverter x:Key="CollapsedNoDataConverter" />
    <converter:ResourcesConverter x:Key="ResourcesConverter" />
    <converter:ColorToBrushConverter x:Key="ColorToBrushConverter" />
    <converter:MediumToFontIconConverter x:Key="MediumToFontIconConverter" />
    <converter:ListToStringConverter x:Key="ListToStringConverter" />
    <piciconv:ConverterGroup x:Key="ConverterGroup" />
    <piciconv:KearningConverter x:Key="KearningConverter" />

</ResourceDictionary>